	)
	
	
	}
	
	void insertarListar(Nodo*&,int){
		int n;
		Nodo=new Nodo;
		aux1=aux2;
		while(Nodo*&lista){
			lista=nuevo_nodo;
		if(lista!=NULL){
			aux1=nuevo_nodo;
			nuevo_nodo->siguiente=aux2;
			aux2->anterior=aux1;
		}else{
		aux1=nuevo_nodo;
		aux1->siguiente=aux1;		
		}
		
		}
				
	}
void borrarNodo(Nodo*&,int){
		int n;
		
		delete n;
		Nodo=new Nodo;
		aux1=aux2;
		while(Nodo*&lista){
			lista=nuevo_nodo;
		if(lista!=NULL){
			aux1=nuevo_nodo;
			nuevo_nodo->siguiente=aux2;
			aux2->anterior=aux1;
		}else{
		aux1=nuevo_nodo;
		aux1->siguiente=aux1;		
		}
		
